        <Button Background="Transparent" BorderThickness="0,0,0,5" BorderBrush="Bisque">
            <TextBlock FontSize="50" TextAlignment="Center" Foreground="White"/>
        </Button>
    </ControlTemplate>
</ResourceDictionary>